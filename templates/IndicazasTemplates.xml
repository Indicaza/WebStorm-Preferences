<templateSet group="IndicazasTemplates">
  <template name="//=" value="//======================================================================================================================" description="Insert comment bar (120chars) //===" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="con" value="console.log($END$)" description="Quick console.log()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fun" value="function $END$() {&#10;&#10;}" description="Creates standard function template" toReformat="false" toShortenFQNames="true">
