<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
  </component>
  <component name="CodeInsightSettings">
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JavaScript.js.type.hints" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="JavaScript.parameter.hints.old" />
    <settingsMapElement>
      <JavaScript.js.type.hints>
        <Settings>
          <option name="showForInferredParameters" value="false" />
        </Settings>
      </JavaScript.js.type.hints>
    </settingsMapElement>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\bNotImplementedException\b" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
  </component>
</application>